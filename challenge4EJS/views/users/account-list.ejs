<%- include('../includes/head.ejs') %>
<body>
<%- include('../includes/navigation.ejs') %>
 <h1>Account List</h1>
 <% if(accts.length > 0) { %>
  <ul>
   <% accts.forEach((aName) => { %>
    <li><%= aName.accountName %></li>
   <% }) %> 
   <% accts.forEach((aNumber) => { %>
    <li><%= aNumber.accountNumber %></li>
   <% }) %> 
  </ul>
 <% } else { %>
  <h1>Account List not found</h1>
 <% } %> 
<%- include('../includes/end.ejs') %>